{
  "nycCoverage": false,
  "coverage": false,
  "auto_compose": true,
  "node": "10.12.0",
  "parallel": 4,
  "services": [
    "redisSentinel",
    "redisCluster",
    "rabbitmq",
    "postgres",
    "elasticsearch",
    "cassandra"
  ],
  "extras": {
    "tester": {
      "working_dir": "/src/packages/core",
      "volumes": [
        "${PWD}/../../:/src"
      ],
      "environment": {
        "TS_NODE_TRANSPILE_ONLY": "true",
        "TS_NODE_TYPE_CHECK": "false",
        "DEBUG": "${DEBUG}"
      }
    }
  }
}
