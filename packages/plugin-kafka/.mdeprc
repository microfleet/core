{
  "nycCoverage": false,
  "coverage": false,
  "auto_compose": true,
  "node": "12",
  "parallel": 3,
  "test_framework": "jest --config ./jest.config.js --runTestsByPath --runInBand",
  "tests": "__tests__/**/*.spec.ts",
  "arbitrary_exec": ["node scripts/rebuild-kafka.js"],
  "sleep": 10,
  "extras": {
    "tester": {
      "working_dir": "/src/packages/plugin-kafka",
      "volumes": [
        "${PWD}/../../:/src:cached"
      ]
    }
  }
}
