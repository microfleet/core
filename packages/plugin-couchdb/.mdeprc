{
  "nycCoverage": false,
  "coverage": false,
  "auto_compose": true,
  "node": "14",
  "parallel": 3,
  "test_framework": "jest --config ../../jest.config.js --runTestsByPath --runInBand",
  "tests": "__tests__/*.spec.ts",
  "services": [
    "couchdb"
  ],
  "extras": {
    "tester": {
      "working_dir": "/src/packages/plugin-couchdb",
      "volumes": [
        "${PWD}/../../:/src:cached"
      ]
    },
    "couchdb": {
      "ports": ["5984:5984"]
    }
  }
}
