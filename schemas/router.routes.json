{
  "id": "router.routes",
  "additionalProperties": false,
  "required": ["directory", "enabled", "prefix", "transports"],
  "type": "object",
  "properties": {
    "directory": {
      "type": "string"
    },
    "enabled": {
      "default": {},
      "patternProperties": {
        "^.*$": {
          "type": "string"
        }
      },
      "type": "object"
    },
    "prefix": {
      "default": "action",
      "type": "string"
    },
    "setTransportsAsDefault": {
      "default": false,
      "type": "boolean"
    },
    "transports": {
      "default": [],
      "items": {
        "type": "string"
      },
      "type": "array",
      "uniqueItems": true
    }
  }
}
